    set {_n} to 0
    loop 3*9 times:
        set slot {_n} of {_p}'s current inventory to gray stained glass pane named "&c"
        add 1 to {_n}
    set slot 10 of {_p}'s current inventory to air
    set slot 11 of {_p}'s current inventory to air
