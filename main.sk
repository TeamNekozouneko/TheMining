                if {join.spam::%player%} is true:
                    if {join.seigen} < 5:
                        set {_l} to random integer between 1 and 4
                        set {_r} to round({_l})
                        if {_r} is 1:
                            broadcast "%player%&a: &f初見さんどもー" 
